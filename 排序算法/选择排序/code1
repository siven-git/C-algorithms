#include <stdio.h>
const int maxn=100;

void selectsort(int A[],int n)
{
    for(int i=0;i<n-1;i++)
    {
        int k=i;
        for(int j=i+1;j<n;j++)
        {
            if(A[j]<A[k])
                k =j;
        }
        int temp=A[k];
        A[k]=A[i];
        A[i]=temp;

    }

}
int main()
{
    int n;
    int A[maxn]={0};
    scanf("%d",&n);
    for(int i=0;i<n;i++)
        scanf("%d",A+i);
    selectsort(A,n);
    for(int i=0;i<n;i++)
        printf("%d",A[i]);

    return 0;
}
